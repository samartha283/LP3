def recursive_fibonacci(n):
    if n <= 1:
        return n
    else:
        return recursive_fibonacci(n - 1) + recursive_fibonacci(n - 2)



if __name__ == "__main__":
    n = int(input("Enter the value of n: "))
    
    print("Recursive Fibonacci:")
    for i in range(n):
        print(recursive_fibonacci(i))

 
